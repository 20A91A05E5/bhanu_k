#example7

n=int(input("enter a number"))
i=1
while i<=n:
           if n%i==0:
                    print(i)
           i=i+1
#example8

n=int(input("enter a number"))
i=1
fact=0
while i<=n:
           if n%i==0:
                    fact=fact+1
           i=i+1
if fact==2:
           print("prime number")
else:
           print("not prime number")

#example9

n=int(input("enter number to be reversed"))
i=1
reverse=0
while n>0:
         rem=n%10
         reverse=reverse*10+rem
         n=n//10
print(reverse)

