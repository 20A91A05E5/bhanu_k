#while loop

i=1
while i<=10:
           print(i)
           i=i+1
#example2

i=10
while i>=1:
           print(i)
           i=i-1
#example3

i=1
sum1=0
while i<=10:
           sum1=sum1+i
           i=i+1
print(sum1)

#example4

