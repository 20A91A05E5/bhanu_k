#if 

a=10
if a>19:
       print("hi")
       print("how are you")
  
#if 2
name=input("enter name")
if name=="bhanu":
               print("valid")
               print("Good")

#if else

a=10
if a>10:
      print("hi")
      print("helloo")
else:
     print("byeee")
     
#if else2

a=10
b=20
if a>b:
      print("a is big")
else:
      print("b is big")

#if else with strings

name=input("enter name")
if name=="csec" or name=="aec":
                          print("valid")
else:
                          print("invalid")
                          

#elif ladder

n=int(input("enter number"))
if n%2==0:
         print("number is divisible by 2")
elif n%3==0:
        print("number is divisible by 3")
elif n%5==0:
        print("number is divisible by 5")
else:
        print("not divisible by2, 3,5")
 
 
 #nested if else
 
name=input("enter name")
if name=="csec":
             password=input("enter password")
             if password=="aec":
                             print("valid")
             else:
                             print("invalid password")

else:
        print("invalid username")

#nested if 2

a=10
b=20
c=30
if a>b:
      if a>c:
           print("a is big")
      else:
           print("c is big")
else:
     if b>c:
           print("b is big ")

     else:
           print(" c is big")

